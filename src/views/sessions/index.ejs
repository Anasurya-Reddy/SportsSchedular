<!DOCTYPE html>
<html>
<head><meta charset="utf-8"><title>Sessions</title><link rel="stylesheet" href="https://cdn.simplecss.org/simple.min.css" /></head>
<body>
	<h2>Sessions</h2>
	<p><a href="/sessions/new">Create Session</a></p>

	<h3>Available Sessions</h3>
	<ul>
		<% available.forEach(ss => { %>
			<li>
				<a href="/sessions/<%= ss._id %>"><%= ss.sportId.name %> - <%= new Date(ss.startsAt).toLocaleString() %> @ <%= ss.venue %></a>
			</li>
		<% }) %>
	</ul>

	<h3>My Sessions</h3>
	<ul>
		<% mine.forEach(ss => { %>
			<li><%= ss.sportId.name %> - <%= new Date(ss.startsAt).toLocaleString() %> @ <%= ss.venue %></li>
		<% }) %>
	</ul>

	<h3>Joined Sessions</h3>
	<ul>
		<% joined.forEach(ss => { %>
			<li><a href="/sessions/<%= ss._id %>"><%= ss.sportId.name %> - <%= new Date(ss.startsAt).toLocaleString() %></a></li>
		<% }) %>
	</ul>

	<p><a href="/">Home</a></p>
</body>
</html>




